<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="eu.vietplus.fahrschul" version="1.0.3" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>eFahrschule</name>
  <description>
        Orignialfragen von TÜV und DEKRA sind nun auch im praktischen App-Format erhältlich. Lernen Sie unterwegs oder auch zuhause und bereiten Sie sich mit einem virtuellen Fahrlehrer optimal auf die Theorieprüfung vor! Zusätzlich werden alle Filmfragen durch zugehörige Filme unterstützt.
    </description>
  <author email="vietplusapp@gmail.com" href="http://vietplus.eu">
        Vietplus Team
    </author>
  <content src="index.html"/>
  <access origin="*"/>
  <preference name="orientation" value="portrait"/>
  <preference name="StatusBarOverlaysWebView" value="true"/>
  <preference name="StatusBarBackgroundColor" value="#181B3D"/>
  <preference name="StatusBarStyle" value="lightcontent"/>
  <preference name="webviewbounce" value="false"/>
  <preference name="UIWebViewBounce" value="false"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="AutoHideSplashScreen" value="false"/>
  <preference name="FadeSplashScreen" value="true"/>
  <preference name="ShowSplashScreenSpinner" value="false"/>
  <preference name="SplashScreenDelay" value="0"/>
  <preference name="FadeSplashScreenDuration" value="500"/>
  <preference name="android-minSdkVersion" value="16"/>
  <preference name="BackupWebStorage" value="none"/>
  <preference name="SplashScreen" value="screen"/>
  <feature name="StatusBar">
    <param name="ios-package" value="CDVStatusBar" onload="true"/>
  </feature>
  <platform name="android">
    <preference name="android-minSdkVersion" value="17" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    <icon density="ldpi" src="res/icons/android/icon-36-ldpi.png" />
    <icon density="mdpi" src="res/icons/android/icon-48-mdpi.png" />
    <icon density="hdpi" src="res/icons/android/icon-72-hdpi.png" />
    <icon density="xhdpi" src="res/icons/android/icon-96-xhdpi.png" />
    <icon density="xxhdpi" src="res/icons/android/icon-144-xxhdpi.png" />
    <icon density="xxxhdpi" src="res/icons/android/icon-192-xxxhdpi.png" />
    <splash density="port-hdpi" src="res/screens/android/screen-hdpi-portrait.png" />
    <splash density="port-ldpi" src="res/screens/android/screen-ldpi-portrait.png" />
    <splash density="port-mdpi" src="res/screens/android/screen-mdpi-portrait.png" />
    <splash density="port-xhdpi" src="res/screens/android/screen-xhdpi-portrait.png" />
  </platform>
  <platform name="ios">
    <preference name="deployment-target" value="7.0" />
    <icon height="114" src="res/icons/ios/Icon@2x.png" width="114" />
    <icon height="80" src="res/icons/ios/Icon-40@2x.png" width="80" />
    <icon height="40" src="res/icons/ios/Icon-40.png" width="40" />
    <icon height="100" src="res/icons/ios/Icon-Small-50@2x.png" width="100" />
    <icon height="50" src="res/icons/ios/Icon-Small-50.png" width="50" />
    <icon height="120" src="res/icons/ios/Icon-40@3x.png" width="120" />
    <icon height="180" src="res/icons/ios/Icon-60@3x.png" width="180" />
    <icon height="60" src="res/icons/ios/Icon-60.png" width="60" />
    <icon height="120" src="res/icons/ios/Icon-60@2x.png" width="120" />
    <icon height="144" src="res/icons/ios/Icon-72@2x.png" width="144" />
    <icon height="72" src="res/icons/ios/Icon-72.png" width="72" />
    <icon height="152" src="res/icons/ios/Icon-76@2x.png" width="152" />
    <icon height="76" src="res/icons/ios/Icon-76.png" width="76" />
    <icon height="167" src="res/icons/ios/Icon-83.5@2x.png" width="167" />
    <icon height="58" src="res/icons/ios/Icon-Small@2x.png" width="58" />
    <icon height="29" src="res/icons/ios/Icon-Small.png" width="29" />
    <icon height="87" src="res/icons/ios/Icon-Small@3x.png" width="87" />
    <icon height="57" src="res/icons/ios/Icon.png" width="57" />
    <icon height="1024" src="res/icons/ios/iTunesArtwork@2x.png" width="1024" />
    <icon height="512" src="res/icons/ios/iTunesArtwork.png" width="512" />
    <splash height="1136" src="res/screens/ios/Default-568h@2x.png" width="640" />
    <splash height="1334" src="res/screens/ios/Default-667h@2x.png" width="750" />
    <splash height="1242" src="res/screens/ios/Default-Landscape-736h@3x.png" width="2208" />
    <splash height="768" src="res/screens/ios/Default-Landscape.png" width="1024" />
    <splash height="1536" src="res/screens/ios/Default-Landscape@2x.png" width="2048" />
    <splash height="2208" src="res/screens/ios/Default-Portrait-736h@3x.png" width="1242" />
    <splash height="1024" src="res/screens/ios/Default-Portrait.png" width="768" />
    <splash height="2048" src="res/screens/ios/Default-Portrait@2x.png" width="1536" />
    <splash height="960" src="res/screens/ios/Default@2x.png" width="640" />
  </platform>
</widget>